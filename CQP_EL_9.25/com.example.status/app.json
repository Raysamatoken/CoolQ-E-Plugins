{
    "ret":1,
    "apiver":9,
    "name":"状态监控",
    "version":"1.0.5",
    "version_id":6,
    "author":"Example",
    "description":"显示酷Q运行状态",
    "event":[
        {
            "id":1001,
            "type":1001,
            "name":"酷Q启动事件",
            "priority":30000,
            "function":"_eventStartup"
        }
    ],
    "status":[        // 悬浮窗状态（将应用运行状态等显示在酷Q悬浮窗）
