            "id":1,                          // 悬浮窗ID
            "name":"运行时间",               // 名称
            "title":"UPTIME",                // 英文名称
            "function":"_statusUptime",      // 数据更新对应函数
            "period":"1000"                  // 更新间隔（单位ms（毫秒），目前仅支持1000ms（1秒））
        },
        {
            "id":2,
            "name":"CPU使用率",
            "title":"CPU",
            "function":"_statusCPU",
            "period":"1000"
        },
        {
            "id":3,
            "name":"当前账号",
            "title":"ACCOUNT",
            "function":"_statusAccount",
            "period":"1000"
        }
    ],
    "auth":[
    ]
}