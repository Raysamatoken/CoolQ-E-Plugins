            "id":1,
            "type":21,
            "name":"私聊消息处理",
            "function":"_eventPrivateMsg",
            "priority":30000,
            "regex":{
                "key":["account","action"],
                "expression":["^(?<action>\\S{1,4}?)\\s*(?<account>\\d{5,10})\\s*?$"]
            }
        },
        {
            "id":2,
            "type":2,
            "name":"群消息处理",
            "function":"_eventGroupMsg",
            "priority":30000,
            "regex":{
                "key":["account","action"],
                "expression":["^(?<action>\\S{1,4}?)\\s*(?<account>\\d{5,10})\\s*?$",
                         "^(?<action>\\S{1,4}?)\\s*\\[CQ:at,qq=(?<account>\\d{5,10})]\\s*?$"]
            }
        },
        {
            "id":3,
